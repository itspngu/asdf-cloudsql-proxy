#!/usr/bin/env bash

: "${ASDF_INSTALL_VERSION:?"Missing ASDF_INSTALL_VERSION"}"
: "${ASDF_DOWNLOAD_PATH:?"Missing ASDF_DOWNLOAD_PATH"}"

# shellcheck source=SCRIPTDIR/lib
. "$(dirname "$0")/lib"

download() {
  curl -fsSL "$(get_download_url)" -o "$ASDF_DOWNLOAD_PATH/bin/cloudsql-proxy"
  exit $?
}

download
